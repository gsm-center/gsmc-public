      , "gulp-concat":          "^2.6.0"
      , "gulp-concat-css":      "^2.3.0"
      , "gulp-data":            "^1.2.1"
      , "gulp-directory-sync":  "^1.2.3"
      , "gulp-exec":            "^2.1.2"
      , "gulp-file-include":    "^0.14.0"
      , "gulp-filter":          "^4.0.0"
      , "gulp-gzip":            "^1.4.0"
      , "gulp-header":          "^1.8.8"
      , "gulp-if":              "^2.0.1"
      , "gulp-inject":          "^4.1.0"
      , "gulp-jscs":            "^4.0.0"
