{   "name":     "gsmc"
  , "title":    "GSMC - Global Services Management Center"
  , "version":  "0.0.1"
  , "description":  "Global Services Management Center based on Laravel Framework and Docker Stack"
  , "author": {
        "name":  "Baltrushaitis Tomas"
      , "email": "tbaltrushaitis@gmail.com"
      , "url":   "https://bitbucket.org/gsmcenter"
    }
  , "license":  "MIT"
  , "private":  true
  , "homepage": "https://bitbucket.org/gsmcenter/public"
  , "website":  "http://gsm-center.com.ua/"
  , "readmeFilename":   "README.md"
  , "engines": {
        "node": ">=0.10.1"
      , "php":  ">=5.5.38"
    }
  , "main": "gulpfile.js"
