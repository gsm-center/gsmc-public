      , "gulp-json-editor":     "^2.2.1"
      , "gulp-livereload":      "^3.8.1"
      , "gulp-newer":           "^1.2.0"
      , "gulp-rename":          "^1.2.2"
      , "gulp-replace":         "^0.5.4"
      , "gulp-sequence":        "^0.4.6"
      , "gulp-tar":             "^1.9.0"
      , "gulp-template":        "^4.0.0"
      , "gulp-uglify":          "^2.0.0"
      , "gulp-util":            "^3.0.7"
      , "gulp-watch":           "^4.3.9"
      , "jshint":               "^2.9.3"
      , "jquery":               "~2.2.4"
      , "jsdom":                "^9.8.3"
      , "jshint-stylish":       "^2.2.1"
      , "lodash":               "~4.17.2"
      , "merge-stream":         "^1.0.0"
      , "minimist":             "^1.2.0"
      , "vinyl-paths":          "^2.1.0"
    }
  , "options": {
        "reporting": {
            "err":    true
          , "stderr": true
          , "stdout": true
        }
      , "exec": {
            "continueOnError":  false
          , "pipeStdout":       false
        }
      , "watch": {
            "ignoreInitial": false
          , "verbose":   true
          , "readDelay": 100
        }
      , "sync": {
            "printSummary": true
          , "nodelete":     true
        }
